{% extends 'layout-admin.html.twig' %}

{% block title 'Gestion du menu' %}

{% set admin_breadcrumb = [
    {label: 'Gestion du menu'}
] %}

{% block content %}
    {% for category in categories %}
        <h3>#{{ category.position }} : <a href="{{ path('admin_menu_category_edit', {id: category.id}) }}">{{ category.name }}</a></h3>
    {% endfor %}
{% endblock %}
