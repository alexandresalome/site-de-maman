<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <title>{% block title '???' %} - {{ sitename }}</title>
        {{ encore_entry_link_tags('app') }}
    </head>
    <body>
        {% block body '' %}
        {{ encore_entry_script_tags('app') }}

        <div id="modal" class="modal fade" tabindex="-1">
            <div class="modal-backdrop">
                <div class="modal-dialog modal-dialog-centered">
                    <div class="modal-content">
                        <div class="modal-body text-center" id="modal-body">
                        </div>
                        <div class="modal-footer">
                            <h5 id="modal-title" class="flex-grow-1">Modal title</h5>
                            <button id="modal-close" type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        {{ include('_analytics.html.twig') }}
    </body>
</html>
